{
  "project": {
    "shortName": "orion",
    "fullName": "ecd.orion",
    "displayName": "Eclipse Orion",
    "sponsorshipLevel": "S0"
  },
  "jenkins": {
    "version": "latest",
    "remotingVersion": "latest",
    "maxConcurrency": 2,
    "permissions": [
      {
        "principal": "anonymous",
        "grantedPermissions": [
          "Overall/Read",
          "Job/Read"
        ]
      },
      {
        "principal": "common",
        "grantedPermissions": [
          "Job/ExtendedRead"
        ]
      },
      {
        "principal": "admins",
        "grantedPermissions": [
          "Overall/Administer"
        ]
      },
      {
        "principal": "ecd.orion",
        "grantedPermissions": [
          "Credentials/View",
          "Gerrit/ManualTrigger",
          "Gerrit/Retrigger",
          "Job/Build",
          "Job/Cancel",
          "Job/Configure",
          "Job/Create",
          "Job/Delete",
          "Job/Move",
          "Job/Read",
          "Job/Workspace",
          "Run/Delete",
          "Run/Replay",
          "Run/Update",
          "View/Configure",
          "View/Create",
          "View/Delete",
          "View/Read",
          "SCM/Tag"
        ]
      }
    ],
    "actualVersion": "2.164.1",
    "actualRemotingVersion": "3.27"
  },
  "docker": {
    "repository": "eclipsecbijenkins",
    "master": {
      "parentImage": "eclipsecbijenkins/jenkins-master-base:latest",
      "image": "eclipsecbijenkins/orion",
      "imageTag": "latest",
      "username": "jenkins",
      "home": "/var/jenkins_home",
      "ref": "/usr/share/jenkins/ref",
      "webroot": "/var/cache/jenkins/war",
      "pluginroot": "/var/cache/jenkins/plugins",
      "war": "/usr/share/jenkins/jenkins.war",
      "updateCenter": "https://updates.jenkins.io",
      "imageSha256": "cb1d60870460e927cfcbaff202a55606a42c34c1e863fdb60a830b82809e080f"
    },
    "agent": {
      "defaultImage": {
        "name": "eclipsecbijenkins/jenkins-agent",
        "tag": "latest",
        "sha256": "13bbe1abbec678a31b8c6df906e6abb24689bf6e3b9f7caaadeaf2850a32d436"
      }
    }
  },
  "deployment": {
    "host": "ci-staging.eclipse.org",
    "prefix": "/orion",
    "url": "https://ci-staging.eclipse.org/orion",
    "uiPort": 8080,
    "jnlpPort": 50000
  },
  "kubernetes": {
    "master": {
      "namespace": "orion",
      "stsName": "orion",
      "resources": {
        "cpu": {
          "request": "500m",
          "limit": 2
        },
        "memory": {
          "limit": "1.5Gi"
        }
      },
      "probe": {
        "liveness": {
          "delaySeconds": 480,
          "periodSeconds": 60,
          "timeoutSeconds": 90,
          "failureThreshold": 2
        },
        "readiness": {
          "delaySeconds": 15,
          "periodSeconds": "60",
          "timeoutSeconds": "90"
        }
      }
    }
  },
  "maven": {
    "home": "/home/jenkins/.m2",
    "files": {
      "settings.xml": {
        "volumeType": "Secret",
        "volumeName": "m2-secret-dir"
      },
      "toolchains.xml": {
        "volumeType": "ConfigMap",
        "volumeName": "m2-dir"
      },
      "settings-security.xml": {
        "volumeType": "Secret",
        "volumeName": "m2-secret-dir"
      }
    }
  }
}
