   ],
   "deployment": {
      "controlPort": 8081,
      "host": "ci-staging.eclipse.org",
      "jnlpPort": 50000,
      "prefix": "/gemini",
      "uiPort": 8080,
      "url": "https://ci-staging.eclipse.org/gemini"
   },
   "docker": {
      "master": {
         "home": "/var/jenkins_home",
         "image": "docker.io/eclipsecbijenkins/gemini",
         "imageTag": "2.222.1",
         "parentImage": "docker.io/eclipsecbijenkins/jenkins-master-base:2.222.1",
         "pluginroot": "/var/cache/jenkins/plugins",
         "ref": "/usr/share/jenkins/ref",
         "updateCenter": "https://updates.jenkins.io",
         "username": "jenkins",
         "war": "/usr/share/jenkins/jenkins.war",
         "webroot": "/var/cache/jenkins/war"
      },
      "repository": "docker.io/eclipsecbijenkins"
   },
   "jenkins": {
      "actualVersion": "2.222.1",
      "agentConnectionTimeout": 180,
      "maxConcurrency": 2,
      "permissions": [
         {
            "grantedPermissions": [
               "Overall/Read",
               "Job/Read"
            ],
            "principal": "anonymous"
         },
         {
            "grantedPermissions": [
               "Job/ExtendedRead"
            ],
            "principal": "common"
         },
         {
            "grantedPermissions": [
               "Overall/Administer"
            ],
            "principal": "admins"
         },
         {
            "grantedPermissions": [
               "Agent/Build",
               "Credentials/View",
               "Job/Build",
               "Job/Cancel",
               "Job/Configure",
               "Job/Create",
               "Job/Delete",
               "Job/Move",
               "Job/Read",
               "Job/Workspace",
               "Overall/Read",
               "Run/Delete",
               "Run/Replay",
               "Run/Update",
               "SCM/Tag",
               "View/Configure",
               "View/Create",
               "View/Delete",
               "View/Read",
               "Gerrit/ManualTrigger",
               "Gerrit/Retrigger"
            ],
            "principal": "rt.gemini"
         }
      ],
      "remotingVersion": "4.2",
      "staticAgentCount": 0,
      "theme": "quicksilver",
      "timezone": "America/Toronto",
      "version": "latest"
   },
   "kubernetes": {
      "agents": {
         "defaultResources": {
            "cpu": {
               "limit": "2000m",
               "request": "1000m"
            },
            "memory": {
               "limit": "4096Mi",
               "request": "4096Mi"
            }
         },
         "namespace": "gemini"
      },
      "master": {
         "namespace": "gemini",
         "probe": {
            "liveness": {
               "delaySeconds": 480,
               "failureThreshold": 5,
               "periodSeconds": 30,
               "timeoutSeconds": 30
            },
            "readiness": {
               "delaySeconds": 1,
               "failureThreshold": 5,
               "periodSeconds": 5,
               "timeoutSeconds": 10
            }
         },
         "resources": {
            "cpu": {
               "limit": "2000m",
               "request": "500m"
            },
            "memory": {
               "limit": "1536Mi",
               "request": "1536Mi"
            }
         },
         "stsName": "gemini"
      }
   },
   "maven": {
      "files": {
         "settings-security.xml": {
            "master": {
               "pass": "bots/rt.gemini/apache-maven-security-settings"
            }
         },
         "settings.xml": {
            "mirrors": {
               "eclipse.maven.central.mirror": {
                  "mirrorOf": "central",
                  "name": "Eclipse Central Proxy",
                  "url": "https://repo.eclipse.org/content/repositories/maven_central/"
               }
            },
            "servers": {
               "gpg.passphrase": {
                  "passphrase": {
                     "pass": "bots/rt.gemini/gpg/passphrase"
                  }
               },
               "ossrh": {
                  "nexusProUrl": "https://oss.sonatype.org",
                  "password": {
                     "pass": "bots/rt.gemini/oss.sonatype.org/password"
                  },
                  "username": {
                     "pass": "bots/rt.gemini/oss.sonatype.org/username"
                  }
               },
               "repo.eclipse.org": {
                  "password": {
                     "pass": "nexus/password"
                  },
                  "username": {
                     "pass": "nexus/username"
                  }
               }
            }
         }
      },
      "generate": true
   },
   "project": {
      "displayName": "Eclipse Gemini",
      "fullName": "rt.gemini",
      "resourcePacks": 1,
      "shortName": "gemini",
      "unixGroupName": "rt.gemini"
   },
   "secrets": {
      "gerrit-trigger-plugin": {
         "identityFile": "/run/secrets/jenkins/ssh/id_rsa",
         "username": "genie.gemini"
      }
   }
}
