               "labels": [ ],
               "maven": {
                  "home": "/home/jenkins/.m2"
               },
               "mode": "NORMAL",
               "name": "basic-agent",
               "remoting": {
                  "jar": "/usr/share/jenkins/agent.jar",
                  "version": "4.2"
               }
            },
