{
  "deployment": {
    "host": "ci.eclipse.org",
    "jnlpPort": 50000,
    "prefix": "/glassfish",
    "uiPort": 8080,
    "url": "https://ci.eclipse.org/glassfish"
  },
  "docker": {
    "agent": {
      "defaultImage": {
        "name": "eclipsecbijenkins/jenkins-agent",
        "sha256": "0d309710872c3a12f22394f2015b8764e039391b2c91801d53ed4f9ef76cb2fe",
        "tag": "3.35"
      }
    },
    "master": {
      "home": "/var/jenkins_home",
      "image": "eclipsecbijenkins/glassfish",
      "imageTag": "2.190.1",
      "parentImage": "eclipsecbijenkins/jenkins-master-base:2.190.1",
      "pluginroot": "/var/cache/jenkins/plugins",
      "ref": "/usr/share/jenkins/ref",
      "updateCenter": "https://updates.jenkins.io",
      "username": "jenkins",
      "war": "/usr/share/jenkins/jenkins.war",
      "webroot": "/var/cache/jenkins/war",
      "imageSha256": "9c575ac58f6e15e1c3695d99487baeb505b13170d595254064683077ea2ed103"
    },
    "repository": "eclipsecbijenkins"
  },
  "jenkins": {
    "actualRemotingVersion": "3.35",
    "actualVersion": "2.190.1",
    "agentConnectionTimeout": 300,
    "maxConcurrency": 30,
    "permissions": [
      {
        "grantedPermissions": [
          "Overall/Read",
          "Job/Read"
        ],
        "principal": "anonymous"
      },
      {
        "grantedPermissions": [
          "Job/ExtendedRead"
        ],
        "principal": "common"
      },
      {
        "grantedPermissions": [
          "Overall/Administer"
        ],
        "principal": "admins"
      },
      {
        "grantedPermissions": [
          "Agent/Build",
          "Credentials/View",
          "Job/Build",
          "Job/Cancel",
          "Job/Configure",
          "Job/Create",
          "Job/Delete",
          "Job/Move",
          "Job/Read",
          "Job/Workspace",
          "Run/Delete",
          "Run/Replay",
          "Run/Update",
          "SCM/Tag",
          "View/Configure",
          "View/Create",
          "View/Delete",
          "View/Read",
          "Gerrit/ManualTrigger",
          "Gerrit/Retrigger"
        ],
        "principal": "ee4j.glassfish"
      }
    ],
    "staticAgentCount": 0,
    "theme": "quicksilver-light",
    "timezone": "America/Toronto",
    "version": "latest"
  },
  "kubernetes": {
    "agents": {
      "defaultResources": {
        "cpu": {
          "limit": "2000m",
          "request": "1000m"
        },
        "memory": {
          "limit": "4096Mi",
          "request": "4096Mi"
        }
      },
      "namespace": "glassfish"
    },
    "master": {
      "namespace": "glassfish",
      "probe": {
        "liveness": {
          "delaySeconds": 480,
          "failureThreshold": 5,
          "periodSeconds": 30,
          "timeoutSeconds": 30
        },
        "readiness": {
          "delaySeconds": 1,
          "failureThreshold": 5,
          "periodSeconds": 5,
          "timeoutSeconds": 10
        }
      },
      "resources": {
        "cpu": {
          "limit": "8000m",
          "request": "4000m"
        },
        "memory": {
          "limit": "8192Mi",
          "request": "8192Mi"
        }
      },
      "stsName": "glassfish"
    }
  },
  "maven": {
    "files": {
      "settings-security.xml": {
        "volumeName": "m2-secret-dir",
        "volumeType": "Secret"
      },
      "settings.xml": {
        "volumeName": "m2-secret-dir",
        "volumeType": "Secret"
      },
      "toolchains.xml": {
        "volumeName": "m2-dir",
        "volumeType": "ConfigMap"
      }
    },
    "home": "/home/jenkins/.m2"
  },
  "project": {
    "displayName": "Eclipse Glassfish",
    "fullName": "ee4j.glassfish",
    "resourcePacks": 15,
    "shortName": "glassfish",
    "unixGroupName": "ee4j.glassfish"
  },
  "secrets": {
    "gerrit-trigger-plugin": {
      "identityFile": "/run/secrets/jenkins/ssh/id_rsa",
      "username": "genie.glassfish"
    }
  }
}
