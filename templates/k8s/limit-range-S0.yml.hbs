apiVersion: "v1"
kind: "LimitRange"
metadata:
  {{> common-metadata }}
  name: "jenkins-instance-limit-range" 
spec:
  limits:
    - type: "Pod"
      {{> limit-range-default-pod }}
      max:
        cpu: 2.5
        memory: 4.5Gi
    - type: "Container"
      {{> limit-range-default-container }}
      max:
        cpu: 2
        memory: 4Gi